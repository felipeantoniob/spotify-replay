{
  "name": "remix",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "@remix-run/serve": "^2.5.1",
    "@spotify-replay/ui": "workspace:*",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@spotify-replay/eslint-config": "workspace:^0.2.0",
    "@spotify-replay/prettier-config": "workspace:^0.1.0",
    "@spotify-replay/store": "workspace:^0.1.0",
    "@spotify-replay/tailwind-config": "workspace:^0.1.0",
    "@spotify-replay/tsconfig": "workspace:^0.1.0",
    "@remix-run/dev": "^2.5.1",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@spotify-replay/eslint-config/base",
      "@spotify-replay/eslint-config/remix",
      "@spotify-replay/eslint-config/react"
    ]
  }
}
